{
  "name": "gameboydisassembler",
  "version": "1.0.3",
  "description": "Disassemble gameboy games",
  "main": "./dist/disassemblerMain.js",
  "scripts": {
    "build": "babel disassembler --presets babel-preset-es2015 --out-dir dist",
    "test": "mocha -w --compilers js:babel-core/register \"./**/*.spec.js\"",
    "integration": "mocha -w --compilers js:babel-core/register \"./disassembler/**/*.integration.js\" \"./roms/**/*.integration.js\"",
    "todos": "mocha -w --compilers js:babel-core/register \"./disassembler/**/*.todo.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amorri40/GameboyDisassembler.git"
  },
  "keywords": [
    "gameboy",
    "z80"
  ],
  "author": "Alasdair Morrison",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/amorri40/GameboyDisassembler/issues"
  },
  "homepage": "https://github.com/amorri40/GameboyDisassembler#readme",
  "devDependencies": {
    "@types/jest": "^20.0.4",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-jest": "^20.0.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babelify": "^7.3.0",
    "chai": "^4.1.1",
    "chai-jest-snapshot": "^1.3.0",
    "jest": "^20.0.4",
    "jest-webpack": "^0.3.0",
    "mochify": "^3.3.0",
    "regenerator-runtime": "^0.10.5",
    "ts-jest": "^20.0.7",
    "typescript": "^2.4.2"
  }
}
